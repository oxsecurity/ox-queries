query GetIssuePrioritization($getIssuesInput: IssuesInput) {
  getIssuePrioritization(getIssuesInput: $getIssuesInput) {      
    originalSeverity {      
      label      
      count    
    }    
    oxPrioritization {
      label      
      count    
    }    
    oxAggregation {
      label
      count    
    } 
  }
}
